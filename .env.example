# MCP Orchestrator Environment Variables
# Copy this file to .env and fill in your API keys

# API Keys (Required)
OPENROUTER_API_KEY=INSERT_YOUR_API_KEY_HERE
OPENAI_API_KEY=INSERT_YOUR_API_KEY_HERE

# Optional API Keys
# ANTHROPIC_API_KEY=INSERT_YOUR_API_KEY_HERE
# GOOGLE_AI_API_KEY=INSERT_YOUR_API_KEY_HERE

# MCP Configuration
MCP_LOG_LEVEL=INFO
MCP_MAX_COST_PER_REQUEST=5.0
MCP_DAILY_LIMIT=100.0

# Security Settings
MCP_ENABLE_ENCRYPTION=true
MCP_MAX_CONCURRENT_TASKS=10